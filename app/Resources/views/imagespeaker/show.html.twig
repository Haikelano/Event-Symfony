{% extends 'base.html.twig' %}

{% block body %}
    <h1>ImageSpeaker</h1>

    <table>
        <tbody>
            <tr>
                <th>Idimagespeaker</th>
                <td>{{ imageSpeaker.idimageSpeaker }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ imageSpeaker.name }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ imageSpeaker.link }}</td>
            </tr>
            <tr>
                <th>Imageprincipal</th>
                <td>{% if imageSpeaker.imagePrincipal %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('imagespeaker_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('imagespeaker_edit', { 'id': imageSpeaker.idimageSpeaker }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
