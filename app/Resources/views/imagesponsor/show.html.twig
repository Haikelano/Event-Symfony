{% extends 'base.html.twig' %}

{% block body %}
    <h1>ImageSponsor</h1>

    <table>
        <tbody>
            <tr>
                <th>Idimagesponsor</th>
                <td>{{ imageSponsor.idimageSponsor }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ imageSponsor.name }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ imageSponsor.link }}</td>
            </tr>
            <tr>
                <th>Imageprinciapl</th>
                <td>{% if imageSponsor.imagePrinciapl %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('imagesponsor_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('imagesponsor_edit', { 'id': imageSponsor.idimageSponsor }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
