{% extends 'base.html.twig' %}

{% block body %}
    <h1>ImageTopics</h1>

    <table>
        <tbody>
            <tr>
                <th>Imagetopicsid</th>
                <td>{{ imageTopic.imagetopicsid }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ imageTopic.link }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('imagetopics_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('imagetopics_edit', { 'id': imageTopic.imagetopicsid }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
